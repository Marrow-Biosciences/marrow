build --bes_results_url=https://marrowbiosciences.buildbuddy.io/invocation/
build --bes_backend=grpcs://marrowbiosciences.buildbuddy.io
build:ci --compilation_mode=opt
build --remote_cache=grpcs://marrowbiosciences.buildbuddy.io
build --experimental_profile_include_primary_output
build --experimental_profile_include_target_label
build --remote_cache_compression
build:ci --remote_header=x-buildbuddy-api-key=${BUILDBUDDY_API_KEY}
build --remote_timeout=10m
build --noslim_profile
build --@rules_rust//rust/settings:extra_rustc_flag=-Zmacro-backtrace
build --@rules_rust//rust/toolchain/channel=nightly
try-import %workspace%/.bazelrc.local
